<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:id="@+id/barOcenki"

    android:layout_height="match_parent">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#fafafa"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:layout_marginBottom="2dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="1dp"
        android:id="@+id/title_bar"
        android:paddingBottom="5dp"
        android:text="@string/lessonTitle"
        android:textSize="19sp" />

    <FrameLayout
        android:id="@+id/frame_ocenki_one"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingRight="1dp"
        android:paddingBottom="2dp"
        android:paddingTop="2dp"
        
        android:paddingLeft="1dp"

        >
        
        <TextView
            android:id="@+id/one"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingBottom="5dp"
            
            android:gravity="center"
            android:background="#fafafa"
            android:text="@string/OneQuarter"
            android:textSize="19sp"
            />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frame_ocenki_two"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingRight="1dp"
        android:paddingBottom="2dp"
        android:paddingTop="2dp"
        
        android:paddingLeft="1dp"

        >
    <TextView
        android:id="@+id/two"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:gravity="center"
        android:paddingBottom="5dp"
        
        android:background="#fafafa"
        android:text="@string/TwoQuarter"
        android:textSize="19sp"
        />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frame_ocenki_three"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingRight="1dp"
        android:paddingBottom="2dp"
        android:paddingTop="2dp"
        
        android:paddingLeft="1dp"

        >
    <TextView
        android:id="@+id/three"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:gravity="center"
        android:paddingBottom="5dp"
        
        android:background="#fafafa"
        android:text="@string/ThreeQuarter"
        android:textSize="19sp"
        />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frame_ocenki_four"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingRight="1dp"
        android:paddingBottom="2dp"
        android:paddingTop="2dp"
        
        android:paddingLeft="1dp"

        >
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:gravity="center"
        android:paddingBottom="5dp"
        android:id="@+id/four"
        android:background="#fafafa"
        android:text="@string/FourQuarter"
        android:textSize="19sp"
        />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frame_ocenki_year"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingRight="1dp"
        android:paddingBottom="2dp"
        android:paddingTop="2dp"
        
        android:paddingLeft="1dp"
        >
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:gravity="center"
        android:paddingBottom="5dp"

        android:id="@+id/year"
        android:text="@string/gradesYear"
        android:background="#fafafa"
        android:textSize="19sp"
        />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frame_ocenki_examination"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingRight="1dp"
        android:paddingBottom="2dp"
        android:paddingTop="2dp"
        
        android:paddingLeft="1dp"
        >
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="5dp"
        android:id="@+id/examination"
        android:gravity="center"
        android:background="#fafafa"
        android:text="@string/gradesExamination"
        android:textSize="19sp"
        />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frame_ocenki_end"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingRight="1dp"
        android:paddingBottom="2dp"
        android:paddingTop="2dp"
        
        android:paddingLeft="1dp"

        >
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="5dp"
        android:gravity="center"
        android:text="@string/gradesEnd"
        android:id="@+id/end"
        android:background="#fafafa"
        android:textSize="19sp"
        />
    </FrameLayout>
</TableRow>